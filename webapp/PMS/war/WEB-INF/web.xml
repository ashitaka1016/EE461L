<?xml version="1.0" encoding="utf-8"?>
 
<web-app xmlns="http://java.sun.com/xml/ns/javaee" version="2.5">
	<servlet>
		<servlet-name>pms</servlet-name>
		<servlet-class>com.example.pms.PMSServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>create</servlet-name>
		<servlet-class>com.example.pms.CreateServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>createSalary</servlet-name>
		<servlet-class>com.example.pms.CreateSalaryServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>createHourly</servlet-name>
		<servlet-class>com.example.pms.CreateHourlyServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>createCommission</servlet-name>
		<servlet-class>com.example.pms.CreateCommissionServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>editCommission</servlet-name>
		<servlet-class>com.example.pms.EditCommissionServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>editHourly</servlet-name>
		<servlet-class>com.example.pms.EditHourlyServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>editSalary</servlet-name>
		<servlet-class>com.example.pms.EditSalaryServlet</servlet-class>
	</servlet>
	
	<servlet-mapping>
		<servlet-name>pms</servlet-name>
		<url-pattern>/pms</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>pms</servlet-name>
		<url-pattern>/signup</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>pms</servlet-name>
		<url-pattern>/login</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>create</servlet-name>
		<url-pattern>/create</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>createSalary</servlet-name>
		<url-pattern>/createsalary</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>createHourly</servlet-name>
		<url-pattern>/createhourly</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>createCommission</servlet-name>
		<url-pattern>/createcommission</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>editCommission</servlet-name>
		<url-pattern>/editcommission</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>editHourly</servlet-name>
		<url-pattern>/edithourly</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>editSalary</servlet-name>
		<url-pattern>/editsalary</url-pattern>
	</servlet-mapping>
	
	<welcome-file-list>
		<welcome-file>pms.jsp</welcome-file>
	</welcome-file-list>
	
	<filter>
        <filter-name>ObjectifyFilter</filter-name>
        <filter-class>com.googlecode.objectify.ObjectifyFilter</filter-class>
	</filter>
	<filter-mapping>
        <filter-name>ObjectifyFilter</filter-name>
        <url-pattern>/*</url-pattern>
	</filter-mapping>
</web-app>
